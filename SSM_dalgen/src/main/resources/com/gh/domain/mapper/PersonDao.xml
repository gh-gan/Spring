<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gh.dao.PersonDao">
	
	<select id="getPersonByName" parameterType="String" resultType="com.gh.domain.PersonDO">
		select	name,age			 
		from person
		where name = #{name} 
	</select>
	
</mapper>